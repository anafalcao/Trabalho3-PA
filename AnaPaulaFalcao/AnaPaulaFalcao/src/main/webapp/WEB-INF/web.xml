<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>eel418.ajaxcombibliotecajson.Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Leitura</servlet-name>
        <servlet-class>eel418.ajaxcombibliotecajson.Leitura</servlet-class>
    </servlet>
        <servlet>
        <servlet-name>MedidorController</servlet-name>
        <servlet-class>eel418.ajaxcombibliotecajson.MedidorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValoresSocket</servlet-name>
        <servlet-class>eel418.ajaxcombibliotecajson.ValoresSocket</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedidorController</servlet-name>
        <url-pattern>/medidorcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Leitura</servlet-name>
        <url-pattern>/leitura</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValoresSocket</servlet-name>
        <url-pattern>/valoresSocket</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
